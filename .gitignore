# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

/node_modules
/yarn-error.log

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore Apple.  Apple silly
*.DS_Store

# Ignore these directories that use rsync
app/assets/images
app/assets/text

# Ignore these directories use for precompiling images for the app
setup/panels

# Just sits on my own computer
purpose.md
